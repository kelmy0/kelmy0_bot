{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "removeComments": true,
    "lib": ["esnext"],
    "incremental": true,
    "sourceMap": false,
    "declaration": false,
    "importHelpers": true,
    "downlevelIteration": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "prisma"]
}
