model User {
    id         String   @id @map("discord_id")
    username   String   @map("discord_username")
    avatar     String   @default("https://cdn.discordapp.com/embed/avatars/0.png") @map("discord_avatar")
    globalName String?  @map("discord_global_name")
    createdAt  DateTime @default(now()) @map("created_at")
    updatedAt  DateTime @updatedAt @map("updated_at")

    // RELATIONS
    addedImages Image[]          @relation("AddedImages")
    categories  ImagesCategory[]

    @@map("users")
}
